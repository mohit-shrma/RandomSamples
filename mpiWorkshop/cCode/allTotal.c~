#include <stdio.h>
#include "mpi.h"

int main(int argc, char **argv) {

  int send[4], recv[3];
  int rank, size, k;

  MPI_Init(&argc, &argv);
  MPI_Comm_rank(MPI_COMM_WORLD, &rank);
  MPI_Comm_size(MPI_COMM_WORLD, &size);

  if (size != 4) {
    printf("Error!:# of processors must be equal to 4\n");
    printf("Aborting...\n");
    MPI_Abort(MPI_COMM_WORLD, 1);
  }

  

  
}
