#include <stdio.h>
#include "mpi.h"
#include <math.h>

int main(int argc, char **argv) {

  int size, rank, iStart, iEnd, i;
  double sum, tmp;
  double a[9];

  MPI_Init(&argc, &argv);
  MPI_Comm_rank(MPI_COMM_WORLD, &rank);
  MPI_Comm_size(MPI_COMM_WORLD, &size);

  //initialize the portion of array corresponding to
  //this rank process
  iStart = rank * 3;
  

  
}
