#include <stdio.h>
#include "mpi.h"
int main(int argc, char **argv) {
  int rank, size, tag, rc, i;
  MPI_Status status;
  int value;
  rc = MPI_Init(&argc, &argv);
  rc = MPI_Comm_size(MPI_COMM_WORLD, &size);
  rc = MPI_Comm_rank(MPI_COMM_WORLD, &rank);
  tag = 100;
  if (rank == 0){
    scanf(%d, &value);
     //send to others
    rc = MPI_Send(message, 13, MPI_CHAR, i, tag, MPI_COMM_WORLD);      
  } else {
    
  }
  
}
